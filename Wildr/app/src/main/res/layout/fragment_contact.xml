<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#67DA6D">

    <ImageView
        android:id="@+id/contactEmail"
        android:layout_width="74dp"
        android:layout_height="79dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="165dp"
        android:layout_marginEnd="321dp"
        android:layout_marginBottom="487dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_email_24" />

    <TextView
        android:id="@+id/emailDetails"
        android:layout_width="228dp"
        android:layout_height="33dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="206dp"
        android:layout_marginEnd="93dp"
        android:layout_marginBottom="506dp"
        android:text="@string/email_wildrapp_wildworld_ie"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/contactEmail"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/contactPhone"
        android:layout_width="80dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="315dp"
        android:layout_marginBottom="403dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactEmail"
        app:srcCompat="@android:drawable/ic_menu_call" />

    <TextView
        android:id="@+id/contactPhoneNumber"
        android:layout_width="185dp"
        android:layout_height="42dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="214dp"
        android:layout_marginBottom="419dp"
        android:text="Phone: 051 9875345"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toEndOf="@+id/contactPhone"
        app:layout_constraintTop_toBottomOf="@+id/emailDetails"
        app:layout_constraintVertical_bias="0.608" />

    <ImageView
        android:id="@+id/contactAddressIcon"
        android:layout_width="87dp"
        android:layout_height="74dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="315dp"
        android:layout_marginBottom="313dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactPhone"
        app:srcCompat="@drawable/ic_baseline_location_city_24" />

    <TextView
        android:id="@+id/contactLocation"
        android:layout_width="208dp"
        android:layout_height="71dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="51dp"
        android:layout_marginBottom="339dp"
        android:text="@string/address_waterford_institute_of_technology_cork_road_waterford"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toEndOf="@+id/contactAddressIcon"
        app:layout_constraintTop_toBottomOf="@+id/contactPhoneNumber"
        app:layout_constraintVertical_bias="0.434" />
</androidx.constraintlayout.widget.ConstraintLayout>